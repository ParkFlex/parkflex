{
    "openapi": "3.1.1",
    "info": {
        "title": "ParkFlex",
        "version": "1.0.0"
    },
    "paths": {
        "/api/user/{id}": {
            "get": {
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ]
            }
        },
        "/api/demo": {
            "put": {
                "summary": "Save provided note to the database.",
                "requestBody": {
                    "description": "",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DemoNoteModel"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": ""
                    },
                    "200": {
                        "description": ""
                    }
                }
            },
            "get": {
                "summary": "Retrieve a note from the database by title.",
                "parameters": [
                    {
                        "name": "title",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DemoNoteModel"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/historyEntry": {
            "get": {
                "parameters": [
                    {
                        "name": "userId",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "422": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/HistoryEntry"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/UserListEntry"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/user/full": {
            "get": {
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/UserListEntry"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "DemoNoteModel": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "contents": {
                        "type": "string"
                    }
                }
            },
            "ApiErrorModel": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "context": {
                        "type": "string"
                    }
                }
            },
            "HistoryEntry": {
                "type": "object",
                "properties": {
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "durationMin": {
                        "type": "integer"
                    },
                    "status": {
                        "type": "string"
                    },
                    "spot": {
                        "type": "integer"
                    }
                }
            },
            "UserListEntry": {
                "type": "object",
                "properties": {
                    "plate": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    },
                    "blocked": {
                        "type": "boolean"
                    },
                    "name": {
                        "type": "string"
                    },
                    "mail": {
                        "type": "string"
                    },
                    "currentPenalty": {
                        "type": "object",
                        "properties": {
                            "reservation": {
                                "type": "integer"
                            },
                            "reason": {
                                "type": "string"
                            },
                            "paid": {
                                "type": "boolean"
                            },
                            "due": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "fine": {
                                "type": "number"
                            }
                        }
                    },
                    "numberOfPastReservations": {
                        "type": "integer"
                    },
                    "numberOfFutureReservations": {
                        "type": "integer"
                    },
                    "numberOfPastBans": {
                        "type": "integer"
                    },
                    "currentReservation": {
                        "type": "boolean"
                    }
                }
            }
        }
    }
}