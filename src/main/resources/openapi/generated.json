{
    "openapi": "3.1.1",
    "info": {
        "title": "ParkFlex",
        "version": "1.0.0"
    },
    "paths": {
        "/api/demo": {
            "put": {
                "summary": "Save provided note to the database.",
                "requestBody": {
                    "description": "",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/DemoNoteModel"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": ""
                    },
                    "200": {
                        "description": ""
                    }
                }
            },
            "get": {
                "summary": "Retrieve a note from the database by title.",
                "parameters": [
                    {
                        "name": "title",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "400": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DemoNoteModel"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/historyEntry": {
            "get": {
                "parameters": [
                    {
                        "name": "userId",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "422": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/HistoryEntry"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/parameter/all": {
            "get": {
                "summary": "Return a list of system parameters.",
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/ParameterModel"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/parameter/{key...}": {
            "get": {
                "summary": "Returns a specified parameter.",
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ParameterModel"
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "summary": "Updates the value of the specified parameter.",
                "requestBody": {
                    "description": "",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ParameterUpdateModel"
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": ""
                    },
                    "400": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ApiErrorModel"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "DemoNoteModel": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "contents": {
                        "type": "string"
                    }
                }
            },
            "ApiErrorModel": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string"
                    },
                    "context": {
                        "type": "string"
                    }
                }
            },
            "HistoryEntry": {
                "type": "object",
                "properties": {
                    "startTime": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "durationMin": {
                        "type": "integer"
                    },
                    "status": {
                        "type": "string"
                    },
                    "spot": {
                        "type": "integer"
                    }
                }
            },
            "ParameterModel": {
                "type": "object",
                "properties": {
                    "key": {
                        "type": "string"
                    },
                    "type": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    },
                    "value": {
                        "type": "string"
                    }
                }
            },
            "ParameterUpdateModel": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "string"
                    }
                }
            }
        }
    }
}